<%= simple_form_for(request,{class: "corners backgroundc"}) do |f| %>
  <%= f.input :service, :collection => ['Groomings', 'Walking', 'Sitting', 'Cuddling'] %>
  <%= f.input :animal %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :animal_info%>
  <%= f.input :housing %>
  <%= f.input :message %>
  <%= f.submit %>
<% end %>
