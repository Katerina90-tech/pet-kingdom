<h1>All Sitters Available</h1>

<ul>
  <% @sitters.each do |sitter| %>
    <% unless sitter == current_user%>
    <li><%= link_to sitter.first_name, sitter_path(sitter) %></li>
    <% end %>
  <%end%>
</ul>


<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>
